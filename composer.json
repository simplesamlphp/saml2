{
    "name": "simplesamlphp/saml2",
    "description": "SAML2 PHP library from SimpleSAMLphp",
    "license": "LGPL-2.1-or-later",
    "authors": [
        {
            "name": "Andreas Ã…kre Solberg",
            "email": "andreas.solberg@uninett.no"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-filter": "*",
        "ext-openssl": "*",
        "ext-pcre": "*",
        "ext-soap": "*",
        "ext-zlib": "*",

        "psr/log": "^2.0 || ^3.0",
        "robrichards/xmlseclibs": "^3.1.1",
        "simplesamlphp/assert": "^1.0.4",
        "simplesamlphp/xml-common": "^1.7.3",
        "simplesamlphp/xml-security": "^1.6.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "simplesamlphp/simplesamlphp-test-framework": "^1.5.4"
    },
    "autoload": {
        "psr-4": {
            "SAML2\\": "src/SAML2"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\SAML2\\": "tests/SAML2/",
            "SAML2\\": "tests/SAML2/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v5.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
