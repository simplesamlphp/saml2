{
    "name": "simplesamlphp/saml2",
    "description": "SAML2 PHP library from SimpleSAMLphp",
    "license": "LGPL-2.1-or-later",
    "authors": [
        {
            "name": "Andreas Åkre Solberg",
            "email": "andreas.solberg@uninett.no"
        },
        {
            "name": "Jaime Pérez Crespo",
            "email": "jaime.perez@uninett.no"
        },
        {
            "name": "Tim van Dijen",
            "email": "tvdijen@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-date": "*",
        "ext-dom": "*",
        "ext-filter": "*",
        "ext-openssl": "*",
        "ext-pcre": "*",
        "ext-soap": "*",
        "ext-zlib": "*",

        "nyholm/psr7": "^1.5.1",
        "psr/clock": "^1.0",
        "psr/http-message": "^2.0",
        "psr/log": "^2.0 || ^3.0",
        "simplesamlphp/xml-common": "^1.11.2",
        "simplesamlphp/xml-security": "^1.6.5",
        "simplesamlphp/xml-soap": "^1.2.1",
        "simplesamlphp/assert": "^1.0.4"
    },
    "require-dev": {
        "beste/clock": "^3.0",
        "mockery/mockery": "^1.5.1",
        "simplesamlphp/simplesamlphp-test-framework": "^1.5.5"
    },
    "autoload": {
        "psr-4": {
            "SimpleSAML\\SAML2\\": "src/SAML2"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimpleSAML\\Test\\SAML2\\": "tests/SAML2/",
            "SimpleSAML\\TestUtils\\SAML2\\": "tests/TestUtils/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "v6.0.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
